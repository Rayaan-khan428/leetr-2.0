{
  "manifest_version": 3,
  "name": "LeetCode Problem Tracker",
  "version": "1.0",
  "description": "Track LeetCode problems with complexity and solutions",
  "permissions": [
    "activeTab",
    "cookies",
    "scripting"
  ],
  "host_permissions": [
    "*://*.leetcode.com/*",
    "http://localhost:3000/*"
  ],
  "content_scripts": [{
    "matches": ["*://*.leetcode.com/*"],
    "js": ["content.js"],
    "css": ["content.css"]
  }],
  "background": {
    "service_worker": "background.js"
  },
  "action": {}
}
