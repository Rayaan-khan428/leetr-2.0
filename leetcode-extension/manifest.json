{
  "manifest_version": 3,
  "name": "LeetCode Problem Tracker",
  "version": "1.0",
  "description": "Track LeetCode problems with complexity and solutions",
  "permissions": ["activeTab", "cookies"],
  "host_permissions": [
    "https://leetcode.com/*",
    "http://localhost:3000/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
